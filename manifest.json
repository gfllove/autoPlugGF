{
"manifest_version":2,
"name":"机器人 正式版",
"description":"自动业绩监控机器人系统",
"version":"1.0.0",
"permissions":[
    "tabs",
    "https://*/*",
    "http://*/*",
    "nativeMessaging", 
    "storage", 
    "activeTab", 
    "cookies",
    "webRequest",
    "webRequestBlocking"
    ],
"browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
    },  
 
 "background": {
      "page": "background.html",
      "persistent": true
   },
   
     "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://key.gf.com.cn/*","https://passport.ly.com/*" ],
      "run_at": "document_end"
   } ]
   

  
  
}